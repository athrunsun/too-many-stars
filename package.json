{
    "name": "oh-my-stars",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "type-check": "tsc -p tsconfig.app.json --noEmit",
        "type-check:watch": "yarn run type-check -- --watch",
        "type-check:test": "tsc -p tsconfig.test.json --noEmit",
        "start": "NODE_ENV=development DEBUG=app:*,eng:* babel-node --config-file ./babel.node.config.js --extensions '.ts' eng/dev/server",
        "start:mock": "ENABLE_MOCK=true yarn run start",
        "build": "yarn run clean && yarn install && yarn run type-check && NODE_ENV=production DEBUG=app:*,eng:* babel-node --config-file ./babel.node.config.js --extensions '.ts' eng/prod/build",
        "serve": "NODE_ENV=production DEBUG=app:*,eng:* babel-node --config-file ./babel.node.config.js --extensions '.ts' eng/prod/serve",
        "serve:dev": "SERVE_PROD_BUNDLE_ON_DEV=true yarn run serve",
        "clean": "rm -rf dist",
        "test": "yarn run type-check:test && jest --coverage",
        "test:watch": "jest --watch",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
        "lint": "eslint --ext .ts,.tsx 'src'"
    },
    "dependencies": {
        "axios": "^0.21.2",
        "bulma": "^0.7.4",
        "lodash": "^4.17.21",
        "path-to-regexp": "^3.0.0",
        "proper-url-join": "^2.0.1",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-router-dom": "^5.0.0",
        "typestyle": "^2.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@types/connect-history-api-fallback": "^1.3.1",
        "@types/copy-webpack-plugin": "^4.4.3",
        "@types/debug": "^0.0.31",
        "@types/dotenv": "^4.0.3",
        "@types/express": "^4.16.0",
        "@types/html-webpack-plugin": "^3.2.0",
        "@types/http-proxy-middleware": "^0.17.6",
        "@types/jest": "^24.0.11",
        "@types/lodash": "^4.14.116",
        "@types/lodash-webpack-plugin": "^0.11.3",
        "@types/mini-css-extract-plugin": "^0.2.0",
        "@types/node": "^10.12.2",
        "@types/react": "^16.8.7",
        "@types/react-dom": "^16.8.2",
        "@types/react-router-dom": "^4.3.1",
        "@types/serve-static": "^1.13.2",
        "@types/webpack": "^4.4.17",
        "@types/webpack-bundle-analyzer": "^2.13.1",
        "@types/webpack-dev-middleware": "^2.0.2",
        "@types/webpack-hot-middleware": "^2.16.4",
        "@types/webpack-merge": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^1.4.2",
        "@typescript-eslint/parser": "^1.4.2",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.5.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-module-resolver": "^3.2.0",
        "connect-history-api-fallback": "^1.5.0",
        "copy-webpack-plugin": "^5.0.2",
        "css-loader": "^1.0.0",
        "debug": "^4.1.0",
        "dotenv": "^6.1.0",
        "eslint": "^5.15.1",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "eslint-plugin-react-hooks": "^1.5.1",
        "express": "^4.16.3",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "http-proxy-middleware": "^0.19.0",
        "jest": "^24.5.0",
        "lodash-webpack-plugin": "^0.11.5",
        "mini-css-extract-plugin": "^0.4.3",
        "prettier": "^1.16.4",
        "serve-static": "^1.13.2",
        "style-loader": "^0.23.0",
        "typescript": "^3.2.4",
        "url-loader": "^1.1.1",
        "webpack": "^4.29.0",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-hot-middleware": "^2.24.2",
        "webpack-merge": "^4.1.4"
    }
}
